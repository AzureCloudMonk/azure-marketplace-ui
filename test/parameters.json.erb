{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "chef_server_url": {
      "value": "<%= params["chef_server_url"] %>"
    },
    "chef_node_name": {
      "value": "<%= params["chef_node_name"] %>"
    },
    "run_list": {
      "value": "<%= params["run_list"] %>"
    },
    "validation_client_name": {
      "value": "<%= params["validation_client_name"] %>"
    },
    "validation_key": {
      "value": <% if params["validation_key"].nil? then %>""<% else %><%= params["validation_key"].to_json %><% end %>
    },
    "client_rb": {
      "value": <% if params["client_rb"].nil? then %>""<% else %><%= params["client_rb"].to_json %><% end %>
    },
    "bootstrap_version": {
      "value": "<%= params["bootstrap_version"] %>"
    },
    "location": {
      "value": "<%= params["location"] %>"
    }
  }
}
